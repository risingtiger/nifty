  },
  "nodemonConfig": {
    "delay": 2500
  },
  "dependencies": {
    "chartist": "^1.3.0",
    "firebase": "^10.13.0",
    "lit-html": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.8.9",
    "brotli": "^1.3.3",
    "esbuild": "0.19.11",
    "eslint": "^9.9.1",
    "fantasticon": "^2.0.0",
    "globals": "^15.9.0",
    "nodemon": "^3.0.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.3.0",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  }
