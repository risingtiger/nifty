{
  "name": "niftyapp",
  "version": "1.0.1",
  "scripts": {
    "build": "./buildit/buildit",
    "dev": "cd server && PORT=3002 NODE_ENV=dev nodemon --watch ./build ./build/index.js",
    "devdebug": "cd server && PORT=3002 NODE_ENV=dev nodemon --inspect ./build/index.js",
    "dist": "cd server && PORT=3003 NODE_ENV=dist node ./build/index.js",
    "distdebug": "cd server && PORT=3003 NODE_ENV=dist nodemon --inspect ./build/index.js"
  },
  "nodemonConfig": {
    "delay": 2500
  },
  "dependencies": {
    "chartist": "^1.3.0",
    "chokidar": "^3.6.0",
    "firebase": "^10.5.2",
    "lit-html": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.8.9",
    "brotli": "^1.3.3",
    "esbuild": "0.19.11",
    "fantasticon": "^2.0.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-nesting": "^12.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  }
}
